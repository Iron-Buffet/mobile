{
    "expo": {
        "sdkVersion": "35.0.0",
        "icon": "./assets/icon2.png",
        "slug": "ironbuffetapp",
        "privacy": "unlisted",
        "loading": {
            "hideExponentText": true,
            "backgroundImage": "./assets/splash.png"
        },
        "orientation": "portrait",
        "primaryColor": "#cccccc",
        "packagerOpts": {
            "sourceExts": [
                "vue",
                "js",
                "json",
                "ts",
                "tsx"
            ],
            "assetExts": [
                "ttf"
            ]
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.ironbuffet.fitnessapp"
        },
        "androidStatusBarColor": "#334393",
        "androidStatusBar": {
            "barStyle": "light-content",
            "backgroundColor": "#334393"
        },
        "name": "Iron Buffet",
        "version": "0.1.0",
        "platforms": [
            "android",
            "ios"
        ]
    }
}
